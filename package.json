{
  "name": "meteorman",
  "version": "1.9.0",
  "author": "Antware",
  "description": "A DDP client with GUI",
  "license": "MIT",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder -wml",
    "build-mac": "node .electron-vue/build.js && electron-builder -m",
    "build-win": "node .electron-vue/build.js && electron-builder -w",
    "build-linux": "node .electron-vue/build.js && electron-builder -l",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": ""
  },
  "build": {
    "productName": "Meteorman",
    "appId": "com.antware.meteorman",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "category": "Utility",
      "icon": "build/icons"
    }
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bufferutil": "^4.0.1",
    "isomorphic-ws": "^4.0.1",
    "json-tree-view": "^0.4.12",
    "markdown-it-vue": "^1.1.6",
    "simpleddp": "^2.2.4",
    "simpleddp-plugin-login": "^4.0.2",
    "utf-8-validate": "^5.0.2",
    "vee-validate": "^3.4.2",
    "vue": "^2.6.12",
    "vue-electron": "^1.0.6",
    "vue-json-editor": "^1.4.2",
    "vue-json-tree-view": "^2.1.6",
    "vue-router": "^3.0.1",
    "vue-simplemde": "^2.0.0",
    "vue-split-panel": "^1.0.4",
    "vuetify": "^2.4.6",
    "vuex": "^3.0.1",
    "vuex-electron": "^1.0.3",
    "ws": "^7.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-stage-0": "^7.8.3",
    "ajv": "^7.0.2",
    "babel-loader": "^8.2.2",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-register": "^6.26.0",
    "cfonts": "^2.9.1",
    "chalk": "^4.1.0",
    "copy-webpack-plugin": "^6.2.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "deepmerge": "^4.2.2",
    "del": "^6.0.0",
    "devtron": "^1.4.0",
    "electron": "^11.1.1",
    "electron-builder": "^22.9.1",
    "electron-debug": "^3.2.0",
    "electron-devtools-installer": "^3.1.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "inject-loader": "^4.0.1",
    "mini-css-extract-plugin": "^1.3.3",
    "multispinner": "^0.2.1",
    "node-loader": "^1.0.2",
    "node-sass": "^4.9.2",
    "require-dir": "^1.0.0",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^4.2.3",
    "url-loader": "^4.1.1",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.9.6",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.43.0",
    "webpack-cli": "^4.3.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^5.7.3"
  }
}
